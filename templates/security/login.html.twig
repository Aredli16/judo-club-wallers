<body>
<div style="background-color: rgb(227, 227, 227); height: 100vh;">
    <div class="container d-flex align-items-center h-100">
        <div class="bg-white tp-shadow5 no-gutters d-flex" style="border-radius: 20px;">
            <div class="row">
                <div class="col-lg-5 d-none d-lg-block">
                    <img
                            src="{{ asset('/uploads/media/albums/content/' ~ image.fileName) }}"
                            class="h-100 img-cover"
                            style="border-radius: 20px 0 0 20px"
                            alt="img"
                    />
                </div>
                <div class="col-lg-7 my-5 text-center">
                    <a href="{{ path('app_home') }}"><img class="w-25 img-fluid m-auto pb-3 "
                                                          src="{{ asset('images/logo.png') }}" alt="logo"></a>
                    <h3 class="py-1">Se Connecter</h3>
                    <form method="post" class="col-lg-7 mx-auto">
                        {% if error %}
                            <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                        {% endif %}

                        {% if app.user %}
                            <div class="mb-3">
                                You are logged in as {{ app.user.userIdentifier }}, <a
                                        href="{{ path('app_logout') }}">Logout</a>
                            </div>
                        {% endif %}
                        <input
                                type="email"
                                value="{{ last_username }}"
                                name="email"
                                id="inputEmail"
                                placeholder="Adresse Email"
                                autocomplete="email"
                                class="form-control my-4 p-2"
                                required autofocus
                        />
                        <input
                                type="password"
                                name="password"
                                id="inputPassword"
                                autocomplete="current-password"
                                placeholder="Mot de passe"
                                class="form-control my-4 p-2"
                                required
                        />
                        <button class="btn btn-primary btn-block">Se connecter</button>
                        <input type="hidden" name="_csrf_token"
                               value="{{ csrf_token('authenticate') }}">
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
